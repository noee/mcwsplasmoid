<?xml version="1.0" encoding="UTF-8"?>
<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0
      http://www.kde.org/standards/kcfg/1.0/kcfg.xsd" >
    <kcfgfile name=""/>

    <group name="Misc">
        <entry name="updateInterval" type="Double">
            <default>2.0</default>
        </entry>
        <entry name="hostList" type="StringList">
            <default>localhost:52199</default>
        </entry>
        <entry name="defaultPort" type="String">
            <default>52199</default>
        </entry>
        <entry name="configChange" type="Bool">
            <default>false</default>
        </entry>
        <entry name="checkZoneChange" type="Bool">
            <default>false</default>
        </entry>
    </group>
    <group name="Playback">
        <entry name="autoShuffle" type="Bool">
            <default>true</default>
        </entry>
        <entry name="shuffleSearch" type="Bool">
            <default>true</default>
        </entry>
        <entry name="forceDisplayView" type="Bool">
            <default>false</default>
        </entry>
        <entry name="showPlayingTrack" type="Bool">
            <default>true</default>
        </entry>
        <entry name="defaultFields" type="String">
            <default>[ { "field": "Name", "sortable": true, "searchable": true, "mandatory": true }
            , { "field": "Artist", "sortable": true, "searchable": true, "mandatory": true }
            , { "field": "Album", "sortable": true, "searchable": true, "mandatory": true }
            , { "field": "Genre", "sortable": true, "searchable": true, "mandatory": true }
            , { "field": "Duration", "sortable": true, "searchable": false, "mandatory": true }
            , { "field": "Media Type", "sortable": false, "searchable": false, "mandatory": true }
            , { "field": "Media Sub Type", "sortable": false, "searchable": false, "mandatory": true }
            , { "field": "Track #", "sortable": true, "searchable": false, "mandatory": true } ]
            </default>
        </entry>
    </group>
    <group name="Appearance">
        <entry name="rightJustify" type="Bool">
            <default>false</default>
        </entry>
        <entry name="showVolumeSlider" type="Bool">
            <default>true</default>
        </entry>
        <entry name="showTrackSlider" type="Bool">
            <default>false</default>
        </entry>
        <entry name="showTrackSplash" type="Bool">
            <default>false</default>
        </entry>
        <entry name="animateTrackSplash" type="Bool">
            <default>false</default>
        </entry>
        <entry name="abbrevZoneView" type="Bool">
            <default>true</default>
        </entry>
        <entry name="abbrevTrackView" type="Bool">
            <default>true</default>
        </entry>
        <entry name="useImageIndicator" type="Bool">
            <default>false</default>
        </entry>
        <entry name="advancedTrayView" type="Bool">
            <default>true</default>
        </entry>
        <entry name="useZoneCount" type="Bool">
            <default>false</default>
        </entry>
        <entry name="trayViewSize" type="Int">
            <default>60</default>
        </entry>
        <entry name="showStopButton" type="Bool">
            <default>false</default>
        </entry>
        <entry name="dropShadows" type="Bool">
            <default>false</default>
        </entry>
        <entry name="highQualityThumbs" type="Bool">
            <default>true</default>
        </entry>
    </group>

</kcfg>
